generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id    Int     @default(autoincrement()) @id
  email String  @unique
  name  String?
}

model Phone {
  id          Int    @default(autoincrement()) @id
  mac_address String @unique
  status String @default("Unknown")
  ip String?
  Room        Room?
  updatedAt DateTime @updatedAt
}

model Room {
  id     Int    @default(autoincrement()) @id
  number String @unique
  phone  Int?   @unique
  Phone  Phone? @relation(fields: [phone], references: [id])
}